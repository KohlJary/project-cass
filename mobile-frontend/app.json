{
  "expo": {
    "name": "Cass",
    "slug": "project-cass",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "updates": {
      "url": "https://u.expo.dev/1ba9ec92-39f3-4615-8b73-c28aba126909"
    },
