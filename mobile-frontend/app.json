      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jaryrigged.projectcass",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-updates",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#7c3aed",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1ba9ec92-39f3-4615-8b73-c28aba126909"
      }
    }
  }
