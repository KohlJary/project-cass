{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_RELAY_URL": "wss://cass-relay-production.up.railway.app"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "credentialsSource": "remote"
      },
      "channel": "preview",
      "env": {
        "EXPO_PUBLIC_RELAY_URL": "wss://cass-relay-production.up.railway.app"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_PUBLIC_RELAY_URL": "wss://cass-relay-production.up.railway.app"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
